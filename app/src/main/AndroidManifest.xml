<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".TaskActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AchieveActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".FishActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".StoreActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".TankActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".CreateTaskDialog"
            android:exported="false"
            android:theme="@style/Theme.CreateTask">
        </activity>
        <activity
            android:name=".AchievementDialog"
            android:exported="false"
            android:theme="@style/Theme.CreateTask">
        </activity>
        <activity
            android:name=".StoreDialog"
            android:exported="false"
            android:theme="@style/Theme.CreateTask">
        </activity>
        <activity
            android:name=".FishDialog"
            android:exported="false"
            android:theme="@style/Theme.CreateTask">
        </activity>
    </application>

</manifest>